<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Button?>

<AnchorPane minHeight="500.0" minWidth="800.0" prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.UserInfoController">
   <children>
      <VBox layoutY="32.0" minHeight="500.0" minWidth="800.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Pane minHeight="100.0" minWidth="800.0" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" layoutX="105.0" layoutY="32.0" text="Информация о пользователе (Редактирование)" textAlignment="CENTER">
                     <font>
                        <Font name="System Bold" size="25.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <HBox minHeight="300.0" minWidth="600.0" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox alignment="CENTER" minHeight="300.0" minWidth="150.0" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label minWidth="100.0" text="Логин:">
                           <font>
                              <Font size="20.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="15.0" left="50.0" right="50.0" top="15.0" />
                           </VBox.margin>
                        </Label>
                        <Label minWidth="100.0" text="Пароль:">
                           <font>
                              <Font size="20.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="15.0" left="50.0" right="50.0" top="15.0" />
                           </VBox.margin>
                        </Label>
                        <Label minWidth="100.0" text="Имя:">
                           <font>
                              <Font size="20.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="15.0" left="50.0" right="50.0" top="15.0" />
                           </VBox.margin>
                        </Label>
                        <Label minWidth="100.0" text="Фамилия:">
                           <font>
                              <Font size="20.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="15.0" left="50.0" right="50.0" top="15.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
                  <VBox minHeight="300.0" minWidth="250.0" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <TextField fx:id="textUserName" minWidth="150.0">
                           <VBox.margin>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="40.0" />
                           </VBox.margin>
                        </TextField>
                        <PasswordField fx:id="textPassword" minWidth="150.0">
                           <VBox.margin>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                           </VBox.margin>
                        </PasswordField>
                        <TextField fx:id="textFirstName" minWidth="150.0">
                           <VBox.margin>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                           </VBox.margin>
                        </TextField>
                        <TextField fx:id="textLastName" minWidth="150.0">
                           <VBox.margin>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                           </VBox.margin>
                        </TextField>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" minHeight="300.0" minWidth="150.0" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label minWidth="100.0" text="Почта:">
                           <font>
                              <Font size="20.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="15.0" left="50.0" right="50.0" top="15.0" />
                           </VBox.margin>
                        </Label>
                        <Label minWidth="100.0" text="Возраст:">
                           <font>
                              <Font size="20.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="15.0" left="50.0" right="50.0" top="15.0" />
                           </VBox.margin>
                        </Label>
                        <Label minWidth="100.0" text="Адрес:">
                           <font>
                              <Font size="20.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="15.0" left="50.0" right="50.0" top="15.0" />
                           </VBox.margin>
                        </Label>
                        <Label minWidth="100.0" text="Пол:">
                           <font>
                              <Font size="20.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="15.0" left="50.0" right="50.0" top="15.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
                  <VBox minHeight="300.0" minWidth="250.0" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <TextField fx:id="textEmail">
                           <VBox.margin>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="40.0" />
                           </VBox.margin>
                        </TextField>
                        <TextField fx:id="textAge">
                           <VBox.margin>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                           </VBox.margin>
                        </TextField>
                        <TextField fx:id="textAdress">
                           <VBox.margin>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                           </VBox.margin>
                        </TextField>
                        <ComboBox fx:id="comboBoxGender" minWidth="220.0" prefHeight="31.0" prefWidth="220.0">
                           <VBox.margin>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                           </VBox.margin>
                        </ComboBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox minHeight="100.0" minWidth="600.0" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="buttonSave" minWidth="100.0" mnemonicParsing="false" onAction="#userSave" text="Сохранить">
                     <HBox.margin>
                        <Insets bottom="70.0" left="200.0" right="100.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="buttonCancle" minWidth="100.0" mnemonicParsing="false" onAction="#registrationCancle" text="Отмена">
                     <HBox.margin>
                        <Insets bottom="70.0" left="100.0" right="200.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
